{
  "name": "tail-database-client",
  "version": "1.0.0",
  "description": "Client for interacting with Tail Database on DataLayer",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest --runInBand test/",
    "prepare": "rm -rf dist && npm run build"
  },
  "keywords": [
    "chia",
    "blockchain",
    "datalayer",
    "tail",
    "database"
  ],
  "author": "Freddie Coleman",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^18.11.9",
    "axios": "^1.1.3",
    "clvm": "^2.0.1",
    "msgpack-lite": "^0.1.26"
  },
  "jest": {
    "roots": [
      "<rootDir>/test",
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "restoreMocks": true,
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "isolatedModules": true
      }
    }
  },
  "devDependencies": {
    "@types/jest": "^29.2.2",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
